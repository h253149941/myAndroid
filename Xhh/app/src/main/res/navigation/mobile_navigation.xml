<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_game">

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.xhh.ui.home.HomeFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_game"
        android:name="com.example.xhh.ui.dashboard.DashboardFragment"
        android:label="@string/title_game"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_weather"
        android:name="com.example.xhh.ui.notifications.NotificationsFragment"
        android:label="@string/title_weather"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_watch"
        android:name="com.example.xhh.ui.watch.WatchFragment"
        android:label="@string/title_watch"
        tools:layout="@layout/watch_fragment" />
</navigation>